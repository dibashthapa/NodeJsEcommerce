<%- include ("includes/header") -%>
<div class="products-list">


<% data.forEach((items)=>{ %>
  <p><%= items.name %></p>
  <% for (i=0;i<items.img_url.length;i++){%>
    <div class="images">
      <img src="<%=items.img_url[i] %>" alt="" height="150px" width="150px">
    </div>
    <div class="images">
<button type="button" name="button"><a href="/cart?types=<%= items.types[i]%>">Add</a></button>

    </div>
    <%}%>
    <% })%>
</div>
<%- include ("includes/footer") -%>
